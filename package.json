{
  "name": "api",
  "version": "1.0.0",
  "description": "12min-challenge - Api",
  "main": "index.js",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "build": "tsc -p .",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:ci": "npm run test -- --coverage",
    "test:verbose": "jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:watch": "npm run test -- --watch",
    "test:watch:ci": "npm run test:ci -- --watch",
    "test:watch:verbose": "npm run test:verbose -- --watch",
    "test:unit": "npm run  test -- --watch -c jest-unit-config.js",
    "test:unit:ci": "npm run test:unit -- --coverage",
    "test:unit:verbose": "npm run  test:verbose -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test -- --watch -c jest-integration-config.js",
    "test:integration:ci": "npm run test:integration -- --coverage",
    "test:integration:verbose": "npm run test:verbose -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci:verbose": "npm run test:verbose -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/leandroluk-12min/api.git"
  },
